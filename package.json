{
  "name": "eldermind",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "scripts": {
    "prepare": "husky",
    "build": "vite build && tsc -b --emitDeclarationOnly",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "doc": "typedoc",
    "test": "vitest --no-watch",
    "coverage": "vitest --coverage --no-watch",
    "make-badges": "istanbul-badges-readme"
  },
  "keywords": [
    "skyrim",
    "skryim platform"
  ],
  "dependencies": {
    "skyrim-effect": "file:../skyrim-effect",
    "@skyrim-platform/skyrim-platform": "2.9.0",
    "dotenv": "^16.4.7",
    "effect": "3.12.5",
    "@effect/platform": "^0.74.0",
    "@effect/platform-node": "^0.70.0",
    "@effect/typeclass": "^0.31.5",
    "@langchain/core": "^0.3.30",
    "@langchain/openai": "^0.3.17",
    "node-microphone": "^0.1.6",
    "marked": "^15.0.6",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "typedoc": "^0.27.6",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.1",
    "@vitest/coverage-v8": "^3.0.1",
    "@effect/vitest": "^0.17.1",
    "@stylistic/eslint-plugin": "^2.13.0",
    "eslint": "^9.18.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-config-prettier": "^10.0.1",
    "@eslint/js": "^9.18.0",
    "prettier": "^3.4.2",
    "typescript-eslint": "^8.20.0",
    "husky": "^9.1.7",
    "glob": "^11.0.1",
    "istanbul-badges-readme": "^1.9.0",
    "@types/node": "^22.10.7",
    "@types/node-microphone": "^0.1.4"
  },
  "exports": {
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./common/Data": {
      "types": "./dist/dts/common/Data.d.ts",
      "import": "./dist/esm/common/Data.js",
      "require": "./dist/cjs/common/Data.js"
    },
    "./common/Error": {
      "types": "./dist/dts/common/Error.d.ts",
      "import": "./dist/esm/common/Error.js",
      "require": "./dist/cjs/common/Error.js"
    },
