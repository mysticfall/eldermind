{
  "name": "eldermind",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "scripts": {
    "prepare": "husky",
    "build": "vite build && tsc -b --emitDeclarationOnly",
    "build:debug": "vite build --mode development && tsc -b --emitDeclarationOnly",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "doc": "typedoc",
    "test": "vitest --no-watch",
    "coverage": "vitest --coverage --no-watch",
    "make-badges": "istanbul-badges-readme"
  },
  "keywords": [
    "skyrim",
    "skryim platform"
  ],
  "dependencies": {
    "skyrim-effect": "file:../skyrim-effect",
    "formdata-node": "^6.0.3",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "marked": "^15.0.11"
  },
  "peerDependencies": {
    "effect": "3.14.14",
    "@effect/platform": "^0.80.14",
    "@effect/platform-node": "^0.77.2",
    "@effect/typeclass": "^0.33.14",
    "@langchain/core": "^0.3.49",
    "@langchain/openai": "^0.5.7",
    "@skyrim-platform/skyrim-platform": "2.9.0"
  },
  "devDependencies": {
    "@effect/vitest": "^0.20.15",
    "@eslint/js": "^9.25.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/node": "^22.15.2",
    "@vitest/coverage-v8": "^3.1.2",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "glob": "^11.0.2",
    "husky": "^9.1.7",
    "istanbul-badges-readme": "^1.9.0",
    "prettier": "^3.5.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "rollup-plugin-external-globals": "^0.13.0",
    "rollup-plugin-node-externals": "^8.0.0",
    "typedoc": "^0.28.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2"
  },
  "exports": {
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
