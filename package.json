    },
    "./speech/Voice": {
      "types": "./dist/dts/speech/Voice.d.ts",
      "import": "./dist/esm/speech/Voice.js",
      "require": "./dist/cjs/speech/Voice.js"
    },
    "./*": {
      "types": "./dist/dts/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    }
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "dist"
  ],
  "pnpm": {
    "overrides": {
      "@skyrim-platform/skyrim-platform": "./node_modules/skyrim-effect/mock/skyrim-platform"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "msgpackr-extract"
    ]
  },
  "typedocOptions": {
    "entryPoints": [
      "./src/index.ts"
    ]
  }
}
